{
  "source": {
    "type": "file_tail",
    "options": {
      "path": "examples/temp/observix/demo.log",
      "from_start": true
    }
  },
  "processor": {
    "mode": "raw",
    "options": {
      "add_fields": {
        "env": "local",
        "service": "demo"
      }
    }
  },
  "destination": {
    "type": "http",
    "options": {
      "url": "http://127.0.0.1:9000/ingest",
      "method": "POST",
      "headers": {
        "Content-Type": "application/json"
      }
    }
  },
  "batch_max_events": 10,
  "batch_max_seconds": 1
}
